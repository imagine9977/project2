<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
"-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Custom">
	<select id="getCustomList" resultType="com.spring1.dto.Custom">
		select * from Custom
	</select>	
	<select id="getStore" resultType="com.spring1.dto.Store">
		select * from stage where storenum=#{storeNum}
	</select>
		<select id="insStorePro" resultType="com.spring1.dto.Store">
		select storenum from (select * from stage order by storenum desc) where rownum=1
	</select>		
	<insert id="insStore">
		insert into stage values (#{storeNum}, #{title}, #{res})
	</insert>
	<update id="upStore">
		update stage set title=#{title} where storenum=#{storeNum}
	</update>
	<delete id="delStore">
		delete from stage where storenum=#{storeNum}
	</delete>
</mapper>